<Window x:Class="JSON_Pokemon__P_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_Pokemon__P_"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFFDAEAE">
        <Label x:Name="lblTitle" Content="Pokemon" HorizontalAlignment="Left" Margin="294,29,0,0" VerticalAlignment="Top" Height="59" Width="190" FontSize="36"/>
        <ComboBox x:Name="cboCharacters" HorizontalAlignment="Left" Margin="86,164,0,0" VerticalAlignment="Top" Width="141" Height="29" SelectionChanged="cboCharacters_SelectionChanged"/>
        <Label x:Name="lblComboBoxLabel" Content="Select A Character" HorizontalAlignment="Left" Margin="86,133,0,0" VerticalAlignment="Top"/>
        <Image x:Name="imgCharacter" HorizontalAlignment="Left" Height="239" Margin="383,93,0,0" VerticalAlignment="Top" Width="268"/>
        <Label x:Name="lblHeightWeight" Content="" HorizontalAlignment="Left" Margin="26,362,0,0" VerticalAlignment="Top" Height="25" Width="730" Background="#FFF7F7B2" FontSize="9"/>

    </Grid>
</Window>
